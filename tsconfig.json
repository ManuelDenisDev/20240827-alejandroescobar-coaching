{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@src/*": ["src/*"],
      "@assets/*": ["src/assets/*"],
      "@components/*": ["src/components/*"],
      "@icons/*": ["src/icons/*"],
      "@images/*": ["src/assets/images/*"],
      "@interfaces/*": ["src/interfaces/*"],
      "@layouts/*": ["src/layouts/*"],
      "@lib/*": ["src/lib/*"],
      "@pages/*": ["src/pages/*"],
      "@styles/*": ["src/styles/*"],
      "@svg/*": ["src/assets/svg/*"],
      "@ui/*": ["src/components/ui/*"],
      "@utils/*": ["src/components/utils/*"],
      "@videos/*": ["src/assets/videos/*"]
    }
  }
}
